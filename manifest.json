{
  "manifest_version": 2,

  "name": "YouTube Suggestion Blocker",
  "description": "Unstuck you from YouTube",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  //   "background": {
  //   "scripts": ["jquery.js", "myscript.js"],
  //   "persistent": false
  // },
  "content_scripts":[
    {
    "matches":["https://www.youtube.com/*"],
    // "matches":["https://*/*"],

    "js":["jquery.js", "content.js"],
    "css": ["content.css"]
  }
  ],
  "permissions": [
  "activeTab"]
}
